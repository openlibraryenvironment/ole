            <xml-property name="queryText"><![CDATA[select
ODBI.TITLE as "Title",
CASE WHEN ODI.CALL_NUMBER IS NOT NULL AND ODI.CALL_NUMBER != ' ' THEN ODI.CALL_NUMBER 
	WHEN ODH.CALL_NUMBER IS NOT NULL AND ODH.CALL_NUMBER != ' ' THEN ODH.CALL_NUMBER 
	ELSE ' ' 
END as "Call Number",
ODI.BARCODE as "Barcode",
CASE WHEN ODI.LOCATION IS NOT NULL AND ODI.LOCATION != ' ' THEN ODI.LOCATION
	WHEN ODH.LOCATION IS NOT NULL AND ODH.LOCATION != ' ' THEN ODH.LOCATION
	ELSE ' ' 
END as "Curent Location",
OCIT.ITM_TYP_NM as "Item Type",
ODIAS.ITEM_AVAIL_STAT_NM as "Item Status",
ODI.DATE_CREATED as "Date Created",
(select MAX(CHECK_IN_DT_TIME) from OLE_DLVR_TEMP_CIRC_RECORD ODTCR
where ODTCR.ITM_ID = ODI.BARCODE) as "Date Last Loaned"
from 
OLE_DLVR_ITEM_AVAIL_STAT_T ODIAS,
OLE_DS_ITEM_T ODI,
OLE_DS_HOLDINGS_T ODH,
OLE_DS_BIB_INFO_T ODBI,
OLE_CAT_ITM_TYP_T OCIT
where
ODIAS.ITEM_AVAIL_STAT_CD = 'LOANED'
and 
ODIAS.ITEM_AVAIL_STAT_ID = ODI.ITEM_STATUS_ID
and
ODI.HOLDINGS_ID =  ODH.HOLDINGS_ID
and
ODBI.BIB_ID = ODH.BIB_ID
and
ODI.ITEM_TYPE_ID = OCIT.ITM_TYP_CD_ID]]></xml-property>
