			<responsibilities>
				<responsibility>
					<role>FISCAL-OFFICER</role>
					<approvePolicy>F</approvePolicy>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
