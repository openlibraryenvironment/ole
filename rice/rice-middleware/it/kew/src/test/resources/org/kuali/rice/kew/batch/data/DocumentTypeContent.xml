			<blanketApproveGroupName namespace="KR-WKFLW">WorkflowAdmin</blanketApproveGroupName>
			<docHandler>http://localhost:8080/en-dev/DocumentType.do?methodToCall=docHandler</docHandler>
			<active>true</active>
		</documentType>
