			<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>
			<description>Sequential Node Test Rule 1</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>bmcgough</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>SeqDocType</documentType>
			<ruleTemplate>Ack1Template</ruleTemplate>
			<description>Sequential Node Ack Rule 1</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>temay</user>
					<actionRequested>K</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>SeqDocType</documentType>
			<ruleTemplate>Ack2Template</ruleTemplate>
			<description>Sequential Node Ack Rule 2</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>jhopf</user>
					<actionRequested>K</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>
			<description>Parallel Node Test Rule 1</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>bmcgough</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>Ack1Template</ruleTemplate>
			<description>Parallel Node Ack Rule 1</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>temay</user>
					<actionRequested>K</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument2Template</ruleTemplate>
			<description>Parallel Node Test Rule 2</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>rkirkend</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument3Template</ruleTemplate>
			<description>Parallel Node Test Rule 3</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>pmckown</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument4Template</ruleTemplate>
			<description>Parallel Node Test Rule 4</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>jitrue</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocument5Template</ruleTemplate>
			<description>Parallel Node Test Rule 5</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>shenl</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ParallelDocType</documentType>
			<ruleTemplate>WorkflowDocumentFinalTemplate</ruleTemplate>
			<description>Parallel Node Test Rule Final</description>
			<forceAction>false</forceAction>
			<responsibilities>
				<responsibility>
					<user>xqi</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
	</rules>
</data>
