<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>BookOrderDocumentType</name>
      <parent>RiceDocument</parent>
      <description>Create a New Book Order Request</description>
      <label>Book Order</label>
      <postProcessorName>org.kuali.rice.krad.workflow.postprocessor.KualiPostProcessor</postProcessorName>
      <superUserGroupName namespace="KR-WKFLW">WorkflowAdmin</superUserGroupName>
      <blanketApproveGroupName namespace="KR-WKFLW">WorkflowAdmin</blanketApproveGroupName>
      <defaultExceptionGroupName namespace="KR-WKFLW">WorkflowAdmin</defaultExceptionGroupName>
      <docHandler>${application.url}/bookOrder.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <routingVersion>2</routingVersion>
    
			<routePaths>
				<routePath>
					<start name="PreRoute" nextNode="Bookstore Master Approval" />
					<role name="Bookstore Master Approval" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="PreRoute">
					<activationType>P</activationType>
				</start>
				<role name="Bookstore Master Approval">
					<qualifierResolverClass>org.kuali.rice.kew.role.NullQualifierResolver</qualifierResolverClass>
					<activationType>P</activationType>
                </role>
			</routeNodes>       
    </documentType>    
  </documentTypes>
</data>