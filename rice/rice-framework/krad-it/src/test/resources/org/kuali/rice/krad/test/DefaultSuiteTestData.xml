    <ruleAttribute>
      <name>TravelAccountDocumentAccountNumberAttribute</name>
      <className>org.kuali.rice.kns.workflow.attribute.KualiXmlSearchableAttributeImpl</className>
      <label>Account Number Attribute</label>
      <description>The search attribute used to find documents by account number.</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="accountNumber" title="kuali_dd_label(TravelAccount)">
          <display>
            <type>text</type>
