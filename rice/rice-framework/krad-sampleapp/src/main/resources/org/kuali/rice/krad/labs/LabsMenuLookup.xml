<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="LabsMenuLookupView" parent="KradSampleAppView">
    <property name="cssClasses">
      <list merge="true">
        <value>demo-kradSampleAppDemo</value>
      </list>
    </property>
    <property name="page.items">
      <list>
        <ref bean="LabsMenuLookups"/>
      </list>
    </property>
  </bean>

  <bean id="LabsMenuLookups" parent="Uif-VerticalBoxSection">
    <property name="items">
      <list>
        <ref bean="LabsMenuLookup-Row1"/>
        <ref bean="LabsMenuLookup-Row2"/>
        <ref bean="LabsMenuLookup-Row3"/>
        <ref bean="LabsMenuLookup-Row4"/>
        <ref bean="LabsMenuLookup-Row5"/>
        <ref bean="LabsMenuLookup-Row6"/>
        <ref bean="LabsMenuLookup-Row7"/>
      </list>
    </property>
  </bean>

